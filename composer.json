{
  "name": "pavsolo/bitrix-stubs",
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "nette/php-generator": "^4.2"
  },

  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mperusso/php-stubs-generator"
    }
